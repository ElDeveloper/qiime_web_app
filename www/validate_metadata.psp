<h2>Metadata Validation</h2>

<%
from metadata_table import *

req.write('<form name=\"metadata_form\" id=\"metadata_form\" method=\"post\" action=\"submit_corrected_metadata.psp\">')

templates = sess["templates"]
if templates != None:
    for template in templates:
        req.write('<h3> Results for file: ' + os.path.basename(template) + '</h3>')
        table = MetadataTable(template)
        req.write(table.printHTMLTable())
    req.write('<p/>')
    
req.write('<p/><input type=\"submit\" value=\"Submit Metadata\" disabled><p/>')
req.write('</form>')
%>
