<div id="content" class="right content">
    <h2>Survey</h2>
    <h4>Personal Information</h4>
    <div class="lefta">
        <table width="100%">
            <tr>
            <td width="30%"></td>
            <td>
                <form id="survey_1">
                    <table width="100%">
                        <tr>
                            <td>Country of birth</td>
                            <td><select></select></td>
                        </tr>
                        <tr>
                            <td>Birth date</td>
                            <td><input type="text" id="datepicker_birth" /></td>
