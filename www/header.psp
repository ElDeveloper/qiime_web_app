<%
# Check to make sure session is valid. If not, redirect to login
s = Session.Session(req)
if not ( s.has_key('username' ) ):
	s.invalidate()
	req.redirect('login.psp')

%>

<html>
<head><title>Qiime</title></head>
<body>

<table width="100%" bgcolor="#CCCCCC">
<tr><td>Qiime<br/>Logged in as <%=s['username']%></td></tr>
</table>
